#define to_action( Actor, Action, VerbAspect=NULL ) \
#begin
#if VerbAspect==NULL
#begin
link ?:Actor{} <action> ?:Action{}
link ?:Action{} <actor> ?:Actor{}
#end
#else
#begin
link ?:Actor{} <action> ?:Action{ ВИД:VerbAspect }
link ?:Action{ ВИД:VerbAspect } <actor> ?:Actor{}
#end
#end

#end

// В некоторых случаях автоматическое распознавание слова может
// дать неверный результат, например ПЕКАРЬ-ПЕЧЬ, в этом случае
// надо явно указать, что следует использовать глагол.
#define to_actionV( Actor, ActionVerb ) \
#begin
link ?:Actor{} <action> Инфинитив:Action{}
link Инфинитив:Action{} <actor> ?:Actor{}
#end

